		<script>
			$(document).ready(function() {
				
				$('.todo-block input[type="checkbox"], .todo-block .prettycheckbox').click(function(){
					$(this).closest('tr').toggleClass('done');
				});
				$('.todo-block input[type="checkbox"]:checked').closest('tr').addClass('done');
				
			});
		</script>
		
	

